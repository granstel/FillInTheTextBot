{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppConfiguration": {
    "HttpLog": {
      "Enabled": true,
      "AddRequestIdHeader": true,
      "ExcludeBodiesWithWords": [ "ping", "pong" ],
      "IncludeEndpoints": [ "sber", "marusia" ]
    },
    "DialogflowScopes": [
      {
        "ProjectId": "%FITB-DF-PROJECTID-0%",
        "LanguageCode": "ru",
        "JsonPath": "%FITB-DF-JSONPATH-0%",
        "LogQuery": false
      }
    ],
    "Redis": {
      "ConnectionString": "",
      "KeyPrefix": ""
    },
    "ExampleWcfService": {
      "Binding": {
        "CloseTimeout": "00:02:00",
        "OpenTimeout": "00:02:00",
        "ReceiveTimeout": "00:10:00",
        "SendTimeout": "00:10:00",
        "MaxReceivedMessageSize": "200000000",
        "MaxBufferSize": "200000000",
        "ReaderQuotas": {
          "MaxArrayLength": "20000000",
          "MaxDepth": "256",
          "MaxBytesPerRead": "20000000"
        },
        "Security": {
          "Mode": "None"
        }
      },
      "Uri": ""
    }
  }
}
