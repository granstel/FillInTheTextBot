{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppConfiguration": {
    "HttpLog": {
      "Enabled": true,
      "AddRequestIdHeader": true,
      "ExcludeBodiesWithWords": [ "ping", "pong" ],
      "IncludeEndpoints": [ "sber", "marusia" ]
    },
    "DialogflowScopes": [
      {
        "ProjectId": "%FITB-DF-PROJECTID-0%",
        "LanguageCode": "ru",
        "JsonPath": "%FITB-DF-JSONPATH-0%",
        "LogQuery": false
      }
    ],
    "Redis": {
      "ConnectionString": "",
      "KeyPrefix": ""
    },
    "Tracing": {
      "Host": "",
      "Port": "" 
    } 
  }
}
